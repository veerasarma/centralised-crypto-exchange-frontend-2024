{
  "name": "b5exchange",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f local.env next dev",
    "clean": "rm -rf ./.next",
    "build": "next build",
    "build:prod": "npm cache clean --force && npm run clean && env-cmd -f prod.env next build && npm run start",
    "build:staging": "npm cache clean --force && npm run clean && env-cmd -f ./dev.env next build && npm run start:staging",
    "start": " next start -p 2053",
    "start:staging": "next start -p 3109 ",
    "lint": "next lint",
    "pages:build_dev": "dotenv -e ./dev.env npx @cloudflare/next-on-pages && npm run clean",
    "pages:build_prod": "dotenv -e ./prod.env npx @cloudflare/next-on-pages && npm run clean",
    "pages:deploy": "npm run pages:build_dev && wrangler deploy --outdir bundled/ --dry-run && wrangler pages deploy .vercel/output/static",
    "pages:watch": "npx @cloudflare/next-on-pages --watch",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-date=2023-12-06 --compatibility-flag=nodejs_compat"
  },
  "dependencies": {
